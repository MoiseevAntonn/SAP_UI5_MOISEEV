<mvc:View
   controllerName="sap.ui.task.controller.Home"
   xmlns="sap.m"
   xmlns:mvc="sap.ui.core.mvc">
   <Page title="{i18n>homePageTitle}" class="sapUiResponsiveContentPadding">
      <content>
         <Button text="{i18n>iWantToNavigate}" class="sapUiTinyMarginEnd"/>
         <Button 
         	id="displayNotFoundBtn" 
         	text="{i18n>DisplayNotFound}"
         	press=".onDisplayNotFound"
         	class="sapUiTinyMarginEnd"/>
         <Button 
         	id="navToRegFormBtn" 
         	text="{i18n>navToRegForm}"
         	press=".onNavToRegForm"
         	class="sapUiTinyMarginEnd"/>
         <Button 
         	id="navToInvListBtn" 
         	text="{i18n>navToInvList}"
         	press=".onNavToInvList"
         	class="sapUiTinyMarginEnd"/>
         <Button id="employeeOverviewBtn" 
         	text="{i18n>ShowEmployeeOverview}" 
         	press=".onNavToEmployeeOverview" 
         	class="sapUiTinyMarginEnd"/>
         <Button id="dataBindingBtn" 
         	text="{i18n>ToDataBinding}" 
         	press=".onNavToDataBinding" 
         	class="sapUiTinyMarginEnd"/>
         <Button id="factFuncBtn" 
         	text="{i18n>ToFactFunc}" 
         	press=".onNavToFactFunc" 
         	class="sapUiTinyMarginEnd"/>
      </content>
   </Page>
</mvc:View>